<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
  version="3.1"
  metadata-complete="true">
  
<servlet>
    <servlet-name>register</servlet-name>
    <jsp-file>/register.jsp</jsp-file>
</servlet>

<servlet>
    <servlet-name>home</servlet-name>
    <jsp-file>/home.jsp</jsp-file>
</servlet>

<servlet>
    <servlet-name>aboutMe</servlet-name>
    <jsp-file>/aboutMe.jsp</jsp-file>
</servlet>

<servlet>
    <servlet-name>breakfast</servlet-name>
    <jsp-file>/breakfast.jsp</jsp-file>
</servlet>

<servlet>
    <servlet-name>lunch</servlet-name>
    <jsp-file>/lunch.jsp</jsp-file>
</servlet>

<servlet>
    <servlet-name>dinner</servlet-name>
    <jsp-file>/dinner.jsp</jsp-file>
</servlet>

<servlet>
    <servlet-name>snack</servlet-name>
    <jsp-file>/snack.jsp</jsp-file>
</servlet>

<servlet>
    <servlet-name>login</servlet-name>
    <jsp-file>/login.jsp</jsp-file>
</servlet>
<servlet>
    <servlet-name>registerDB</servlet-name>
    <servlet-class>InsertData</servlet-class>
</servlet>

<servlet>
    <servlet-name>loginDB</servlet-name>
    <servlet-class>check</servlet-class>
</servlet>

<servlet>
    <servlet-name>Entries</servlet-name>
    <servlet-class>Entries</servlet-class>
</servlet>


<servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
</servlet-mapping>

<servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home</url-pattern>
</servlet-mapping>

<servlet-mapping>
    <servlet-name>breakfast</servlet-name>
    <url-pattern>/breakfast</url-pattern>
</servlet-mapping>

<servlet-mapping>
    <servlet-name>lunch</servlet-name>
    <url-pattern>/lunch</url-pattern>
</servlet-mapping>

<servlet-mapping>
    <servlet-name>dinner</servlet-name>
    <url-pattern>/dinner</url-pattern>
</servlet-mapping>

<servlet-mapping>
    <servlet-name>snack</servlet-name>
    <url-pattern>/snack</url-pattern>
</servlet-mapping>

<servlet-mapping>
    <servlet-name>aboutMe</servlet-name>
    <url-pattern>/aboutMe</url-pattern>
</servlet-mapping>


<servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/register</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>registerDB</servlet-name>
    <url-pattern>/registerDB</url-pattern>
</servlet-mapping>
 
<servlet-mapping>
    <servlet-name>loginDB</servlet-name>
    <url-pattern>/loginDB</url-pattern>
</servlet-mapping>

 
<servlet-mapping>
    <servlet-name>Entries</servlet-name>
    <url-pattern>/Entries</url-pattern>
</servlet-mapping>

</web-app>


